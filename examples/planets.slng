Type Planet
	radius: Number
	mass: Number
	position: Vector	
	position: position + velocity*Time
	velocity: Vector
	velocity: velocity + acceleration*Time
	force: Vector
	acceleration: force/mass

Type System
	Set Planet planets
	# [p In planets]
		p.force = sum([other In (planets-p)] 6.676E-11*p.mass*other.mass/distance(p.position,other.position)^2)
